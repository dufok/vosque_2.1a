{
    "name": "myapp-monorepo",
    "private": true,
    "workspaces": [
      "apps/*",
      "packages/*"
    ],
    "packageManager": "bun@1.0.0",
    "prisma": {
      "schema": "./packages/db/prisma/schema.prisma",
      "seed": "ts-node ./packages/db/seed/seed.ts",
      "upseed": "ts-node ./packages/db/seed/upseed.ts"
    },
    "scripts": {
      "dev": "cd apps/nextjs && bun with-env next dev",
      "build": "cd apps/nextjs && bun with-env next build",
      "clean": "rm -rf node_modules",
      "upseed": "ts-node ./packages/db/seed/upseed.ts",
      "generate": "cd packages/db && bun prisma generate",
      "build:ui": "cd packages/ui && bun build",
      "studio": " cd packages/db && bun prisma:studio"
    },
    "resolutions": {
      "esbuild": "0.15.6",
      "@types/react": "17.0.21",
      "react": "18.2.0",
      "react-dom": "18.2.0",
      "react-refresh": "^0.14.0",
      "react-native-svg": "13.4.0",
      "react-native-web": "0.18.9",
      "redux": "^4.2.1",
      "video-react": "^0.16.0"
    },
    "dependencies": {
      "@babel/runtime": "^7.18.9",
      "@tamagui/animations-css": "^1.39.5",
      "@tamagui/collection": "^1.39.5",
      "@vercel/analytics": "^1.0.2",
      "dotenv": "^16.0.3",
      "dotenv-cli": "^6.0.0"
    },
    "devDependencies": {
      "eslint": "^8.21.0",
      "lerna-update-wizard": "^1.1.2",
      "prettier": "^2.7.1",
      "ts-node": "^10.9.1",
      "typescript": "^4.9.4"
    }
  }
  